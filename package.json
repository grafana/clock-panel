{
  "name": "clock-panel",
  "version": "2.1.7",
  "description": "Clock Panel Plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "playwright:test": "npx playwright test",
    "playwright:test:ui": "npx playwright test --ui",
    "playwright:showreport": "npx playwright show-report",
    "server": "docker compose up --build",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "keywords": [
    "clock",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Grafana Labs",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "^1.7.1",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@playwright/test": "^1.42.1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.8.7",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "8.8.4",
    "css-loader": "^6.7.3",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lodash": "latest",
    "moment": "2.29.4",
    "prettier": "^3.3.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-virtual-modules": "^0.6.2"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.4.8",
    "@grafana/runtime": "10.4.8",
    "@grafana/schema": "10.4.8",
    "@grafana/ui": "10.4.8",
    "moment-timezone": "0.5.38",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.5.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "moment": "2.29.4",
    "moment-timezone": "0.5.38",
    "@types/react": "17.0.30"
  }
}
