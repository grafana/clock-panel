{
  "name": "clock-panel",
  "version": "3.0.0",
  "description": "Clock Panel Plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "format": "prettier --write ./src",
    "i18n-extract": "i18next-cli extract --sync-primary",
    "i18n-extract:ci": "i18next-cli extract --sync-primary --dry-run --ci",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "playwright:test": "npx playwright test",
    "playwright:test:ui": "npx playwright test --ui",
    "playwright:showreport": "npx playwright show-report",
    "server": "docker compose up --build",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "keywords": [
    "clock",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Grafana Labs",
  "license": "MIT",
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/eslint-plugin-plugins": "0.5.2",
    "@grafana/plugin-e2e": "^3.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "1.56.0",
    "@stylistic/eslint-plugin-ts": "4.4.1",
    "@swc/core": "1.13.20",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "0.2.39",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.2",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "copy-webpack-plugin": "13.0.1",
    "cspell": "9.2.1",
    "css-loader": "7.1.2",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsdoc": "61.1.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-webpack-plugin": "5.0.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "glob": "11.0.3",
    "i18next-cli": "^1.11.4",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "moment": "2.30.1",
    "prettier": "3.6.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.93.2",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0",
    "swc-loader": "0.2.6",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.2.0",
    "@grafana/i18n": "^12.2.0",
    "@grafana/runtime": "^12.2.0",
    "@grafana/schema": "^12.2.0",
    "@grafana/ui": "^12.2.0",
    "moment-timezone": "0.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "semver": "^7.7.3"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "npm@11.6.2",
  "overrides": {
    "moment": "2.30.1"
  }
}
