{
  "name": "clock-panel",
  "version": "2.1.9",
  "description": "Clock Panel Plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "format": "prettier --write ./src",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "playwright:test": "npx playwright test",
    "playwright:test:ui": "npx playwright test --ui",
    "playwright:showreport": "npx playwright show-report",
    "server": "docker compose up --build",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "keywords": [
    "clock",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Grafana Labs",
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@grafana/eslint-config": "8.2.0",
    "@grafana/eslint-plugin-plugins": "0.4.7",
    "@grafana/plugin-e2e": "2.1.13",
    "@grafana/tsconfig": "2.0.0",
    "@playwright/test": "1.55.0",
    "@stylistic/eslint-plugin-ts": "4.4.1",
    "@swc/core": "1.13.5",
    "@swc/jest": "0.2.39",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.3",
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "copy-webpack-plugin": "13.0.1",
    "cspell": "9.2.1",
    "css-loader": "7.1.2",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsdoc": "55.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-webpack-plugin": "5.0.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "glob": "11.0.3",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "5.0.0",
    "jest": "30.1.3",
    "jest-environment-jsdom": "30.1.2",
    "moment": "2.30.1",
    "prettier": "3.6.2",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.92.1",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0",
    "swc-loader": "0.2.6",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "webpack": "5.101.3",
    "webpack-cli": "6.0.1",
    "webpack-livereload-plugin": "3.0.2",
    "webpack-subresource-integrity": "5.1.0",
    "webpack-virtual-modules": "0.6.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.0.2",
    "@grafana/runtime": "^12.1.0",
    "@grafana/ui": "^12.2.0",
    "moment-timezone": "0.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@11.6.0",
  "overrides": {
    "moment": "2.30.1"
  }
}
