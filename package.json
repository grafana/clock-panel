{
  "name": "clock-panel",
  "version": "2.1.8",
  "description": "Clock Panel Plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "format": "prettier --write ./src",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "playwright:test": "npx playwright test",
    "playwright:test:ui": "npx playwright test --ui",
    "playwright:showreport": "npx playwright show-report",
    "server": "docker compose up --build",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "keywords": [
    "clock",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Grafana Labs",
  "license": "MIT",
  "devDependencies": {
    "@grafana/eslint-config": "8.0.0",
    "@grafana/eslint-plugin-plugins": "0.4.3",
    "@grafana/plugin-e2e": "2.1.6",
    "@grafana/tsconfig": "2.0.0",
    "@playwright/test": "1.54.1",
    "@stylistic/eslint-plugin-ts": "2.11.0",
    "@swc/core": "1.12.9",
    "@swc/jest": "0.2.37",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.5",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "copy-webpack-plugin": "13.0.0",
    "cspell": "8.18.1",
    "css-loader": "7.1.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-webpack-plugin": "4.2.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "glob": "10.4.1",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "5.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "30.0.5",
    "moment": "2.30.1",
    "prettier": "3.5.3",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.63.2",
    "sass-loader": "16.0.5",
    "style-loader": "3.3.3",
    "swc-loader": "0.2.6",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "webpack": "5.97.1",
    "webpack-cli": "5.1.4",
    "webpack-livereload-plugin": "3.0.2",
    "webpack-subresource-integrity": "5.1.0",
    "webpack-virtual-modules": "0.6.2",
    "@swc/helpers": "0.5.11",
    "semver": "7.7.1"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.0.2",
    "@grafana/runtime": "^12.0.2",
    "@grafana/ui": "^12.0.2",
    "moment-timezone": "0.5.47",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@grafana/schema": "^12.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "moment": "2.30.1",
    "moment-timezone": "0.5.47",
    "@types/react": "17.0.30"
  }
}
