{
  "name": "clock-panel",
  "version": "2.1.8",
  "description": "Clock Panel Plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "format": "prettier --write ./src",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "playwright:test": "npx playwright test",
    "playwright:test:ui": "npx playwright test --ui",
    "playwright:showreport": "npx playwright show-report",
    "server": "docker compose up --build",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "keywords": [
    "clock",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Grafana Labs",
  "license": "MIT",
  "devDependencies": {
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/eslint-plugin-plugins": "^0.2.1",
    "@grafana/plugin-e2e": "^1.17.1",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.49.1",
    "@stylistic/eslint-plugin-ts": "^2.11.0",
    "@swc/core": "^1.11.5",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.7",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "copy-webpack-plugin": "^13.0.0",
    "cspell": "8.8.4",
    "css-loader": "^6.7.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "moment": "2.29.4",
    "prettier": "^3.4.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "16.0.4",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.4",
    "terser-webpack-plugin": "^5.3.11",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^11.3.1",
    "@grafana/runtime": "^11.3.0",
    "@grafana/ui": "^11.5.1",
    "moment-timezone": "0.5.47",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "7.2.0"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "moment": "2.29.4",
    "moment-timezone": "0.5.47",
    "@types/react": "17.0.30"
  }
}
